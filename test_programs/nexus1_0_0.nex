function int entry() {
    var int sum = 0;
    var int i = 0;

    while (i < 10) {
        if (i == 5) {
            i = i + 1;
            continue;
        }

        if (i == 8) {
            break;
        }

        sum = sum + i;
        i = i + 1;
    }

    if (sum > 20 && sum < 40) {
        return sum;
    } else {
        return -1;
    }
}
